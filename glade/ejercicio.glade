<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="3.0"/>
  <object class="GtkWindow" id="main">
    <property name="title">Inicio de Sesión</property>
    <property name="default_width">400</property>
    <property name="default_height">300</property>
    <property name="visible">True</property>
    <child>
      <object class="GtkBox" id="box_main">
        <property name="orientation">vertical</property>
        <property name="spacing">10</property>
        <property name="margin">20</property>
        <property name="visible">True</property>
        <child>
          <object class="GtkLabel" id="label1">
            <property name="label">Correo electrónico</property>
            <property name="visible">True</property>
          </object>
        </child>
        <child>
          <object class="GtkEntry" id="entry1">
            <property name="visible">True</property>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="label2">
            <property name="label">Contraseña</property>
            <property name="visible">True</property>
          </object>
        </child>
        <child>
          <object class="GtkEntry" id="entry2">
            <property name="visibility">False</property>
            <property name="visible">True</property>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="box_recover">
            <property name="orientation">horizontal</property>
            <property name="spacing">10</property>
            <property name="visible">True</property>
            <child>
              <object class="GtkLabel" id="label3">
                <property name="label">¿Olvidó su contraseña? Haga clic aquí:</property>
                <property name="visible">True</property>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="reset_button">
                <property name="label">Recuperar</property>
                <property name="visible">True</property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkButton" id="save_button">
            <property name="label">Guardar</property>
            <property name="visible">True</property>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
